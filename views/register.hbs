<h1>{{title}}</h1>
<p>{{message}}</p>
<form action="/users" method="POST">
  <div>
    <label>Username</label>
    <input type="text" name="username">
  </div>
  <div>
    <label>Bio</label>
    <textarea name="bio"></textarea>
  </div>
  <div>
    <label>Password</label>
    <input type="password" name="password">
  </div>
  <input type="submit">
</form>
